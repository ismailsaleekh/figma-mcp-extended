{
  "name": "figma-mcp-extended",
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=code.js --format=iife --target=es6",
    "watch": "esbuild src/index.ts --bundle --outfile=code.js --format=iife --target=es6 --watch",
    "dev": "npm run watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "dependencies": {
    "svg-path-parser": "^1.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0",
    "@figma/plugin-typings": "^1.100.0"
  }
}
